   19  ifconfig
   20  netstat -antup
   21  netstat -plantux
   22  netstat -nap
   23  route
   24  arp -a
   25  lsof -i
   26  #  "SERVICE INFORMATION"
   27  ps -aux
   28  lsmod
   29  lsof
   30  #  "FILES, DRIVES AND SHARES INFORMATION"
   31  df -ah
   32  ls -la /etc/init.d
   33  stat -x /etc/init.d/  
   34  file /etc/init.d/     
   35  lsattr -R / | grep "\-i\-"
