    1  # "USER INFORMATION"
    2  w
    3  lastlog
    4  last
    5  faillog -a
    6  cat /etc/passwd
    7  cat /etc/shadow
    8  cat /etc/group
    9  cat /etc/sudoers
   10  awk -F: '($3 == "0") {print}' /etc/passwd
   11  egrep ':0+' /etc/passwd
   12  #  "SYSTEM INFORMATION"
   13  uname -a
   14  uptime
   15  timedatectl
   16  mount
   17  echo $PATH
